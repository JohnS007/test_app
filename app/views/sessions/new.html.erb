<h1 align = "center">Log In</h1>
<%= form_for(:sessions, url: login_path)do |f| %>
  <p>
    <%= f.label :email %> <br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password %> <br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Log In"%>
  </p>

<% end %>
<%= link_to "Back to Listing", articles_path %>
